<!-- ISO METADATA CONTENT MODEL -->
<!-- Content model for the element <iso-meta> -->
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <define name="iso-meta-model">
    <zeroOrMore>
      <ref name="title-wrap"/>
    </zeroOrMore>
    <optional>
      <ref name="doc-ident"/>
    </optional>
    <optional>
      <ref name="std-ident"/>
    </optional>
    <zeroOrMore>
      <choice>
        <ref name="std-org"/>
        <ref name="std-org-group"/>
      </choice>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="content-language"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="std-ref"/>
    </zeroOrMore>
    <optional>
      <ref name="doc-ref"/>
    </optional>
    <optional>
      <ref name="pub-date"/>
    </optional>
    <zeroOrMore>
      <ref name="release-date"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="meta-date"/>
    </zeroOrMore>
    <zeroOrMore>
      <choice>
        <ref name="comm-ref"/>
        <ref name="comm-ref-group"/>
      </choice>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="secretariat"/>
    </zeroOrMore>
    <zeroOrMore>
      <choice>
        <ref name="ics"/>
        <ref name="ics-wrap"/>
      </choice>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="subj-group"/>
    </zeroOrMore>
    <optional>
      <ref name="page-count"/>
    </optional>
    <optional>
      <ref name="counts"/>
    </optional>
    <optional>
      <ref name="is-proof"/>
    </optional>
    <zeroOrMore>
      <ref name="std-xref"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="permissions"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="self-uri"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="abstract.class"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="kwd-group.class"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="meta-note"/>
    </zeroOrMore>
    <zeroOrMore>
      <ref name="custom-meta-group"/>
    </zeroOrMore>
  </define>
  <!-- ISO METADATA ATTRIBUTES -->
  <define name="iso-meta-atts">
    <ref name="jats-common-atts"/>
  </define>
  <!-- ISO METADATA -->
  <!--
    The <iso-meta> is a container element for
    ISO-specific metadata for this standard.
       REMARKS: This element contains any
    ISO-specific overrides to the metadata 
    described for the whole standards 
    document (<std-doc-meta>) as well as any
    additional ISO-specific metadata. The element 
    <iso-meta> is the 4th of the organizational 
    metadata elements with <std-meta>, 
    <nat-meta>, and <reg-meta>.
  -->
  <define name="iso-meta">
    <element name="iso-meta">
      <ref name="iso-meta-attlist"/>
      <ref name="iso-meta-model"/>
    </element>
  </define>
  <define name="iso-meta-attlist" combine="interleave">
    <ref name="iso-meta-atts"/>
  </define>
</grammar>
